{
  "name": "grafana-demo-ecommerce",
  "version": "1.0.0",
  "description": "Ecommerce application for Grafana Cloud demonstration",
  "main": "./src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
