    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.2",
    "pino": "^9.9.4",
    "pino-http": "^10.5.0",
    "pino-opentelemetry-transport": "^1.0.1",
    "prom-client": "^15.1.0",
    "uuid": "^9.0.1"
